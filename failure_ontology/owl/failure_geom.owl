        <rdfs:label>Geometric component</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/geom.owl#GeometricFailureDiagnosis -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/geom.owl#GeometricFailureDiagnosis">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/geom.owl#FailureDiagnosis"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/geom.owl#describesAsCause"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/geom.owl#GeometricComponent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/geom.owl#describesAsCause"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/geom.owl#GeometricComponent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Geometric failure diagnosis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/geom.owl#GeometricFeasibilityChecker -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/geom.owl#GeometricFeasibilityChecker">
